{
  "name": "project-mean",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build --configuration development",
    "build:integrated": "ng build --configuration production-integrated",
    "build:separate": "ng build --configuration production-separate && npm --prefix backend/build/backend/ninja run build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:server": "set NODE_ENV=development&& nodemon server.js",
    "start:prod": "set NODE_ENV=production-integrated&& nodemon server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.3",
    "@angular/cdk": "^12.2.3",
    "@angular/common": "~12.2.3",
    "@angular/compiler": "~12.2.3",
    "@angular/core": "~12.2.3",
    "@angular/forms": "~12.2.3",
    "@angular/material": "^12.2.3",
    "@angular/platform-browser": "~12.2.3",
    "@angular/platform-browser-dynamic": "~12.2.3",
    "@angular/router": "~12.2.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "mongoose": "^6.0.7",
    "pug": "^3.0.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.3",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "nodemon": "^2.0.12",
    "typescript": "~4.3.2"
  }
}
